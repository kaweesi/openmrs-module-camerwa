<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="org.openmrs.module.camerwa">

	<class name="RegimenComposition" table="regimenComposition">

		<id name="regimenId" column="regimen_id">
			<generator class="native" />
		</id>

		<property name="regimenName" column="regimen_name " />
		<property name="regimenCategory" column="regimen_category " />
		<property name="drugConceptId1" column="drug_concept_id1 " />
		<property name="drugConceptId2" column="drug_concept_id2 " />
		<property name="drugConceptId3" column="drug_concept_id3 " />	
	    <property name="drugConceptId4" column="drug_concept_id4 " />
	</class>

</hibernate-mapping>
